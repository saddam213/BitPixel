@model DotMatrix.Common.Pixel.PixelViewlModel
@{
	ViewBag.Title = "Home";
}

<div class="row text-center">
	<table style="border:solid 1px black;margin: 10px auto;">
		@for (int y = 0; y < 4; y++)
		{
			<tr>
				<td><img src="@string.Format("~/Content/Images/background-small-{0}.png", 0 + (y * 4))" width="250" height="250"  /></td>
				<td><img src="@string.Format("~/Content/Images/background-small-{0}.png", 1 + (y * 4))" width="250" height="250"  /></td>
				<td><img src="@string.Format("~/Content/Images/background-small-{0}.png", 2 + (y * 4))" width="250" height="250"  /></td>
				<td><img src="@string.Format("~/Content/Images/background-small-{0}.png", 3 + (y * 4))" width="250" height="250"  /></td>
			</tr>
		}
	</table>
</div>
<hr />
<footer>
	<p>&copy; @DateTime.Now.Year - My Cryptopia Application</p>
</footer>
@section Scripts {
	<script>
	</script>
}
